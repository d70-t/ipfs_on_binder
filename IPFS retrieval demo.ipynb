    "\n",
    "class IPFSJpeg:\n",
    "    def __init__(self, cid):\n",
    "        self.cid = cid\n",
    "\n",
    "    def _repr_jpeg_(self):\n",
    "        with fsspec.open(f\"ipfs://{self.cid}\") as of:\n",
    "            return of.read()"
